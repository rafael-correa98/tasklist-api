{
  "name": "tasklist-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main/server.ts",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/main/server.ts",
    "build": "npx tsc",
    "start": "node ./dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:up": "typeorm-ts-node-commonjs migration:run -d ./src/main/database/database-connection.ts",
    "typeorm:down": "typeorm-ts-node-commonjs migration:revert -d ./src/main/database/database-connection.ts",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/main/database/database-connection.ts",
    "test": "jest --silent --noStackTrace --runInBand",
    "test:verbose": "jest --verbose --runInBand --detectOpenHandles",
    "test:coverage": "jest --runInBand --coverage",
    "test:watch": "jest --silent --noStackTrace --runInBand --watchAll",
    "test:unit": "jest --c jest.unit.config.ts --silent --noStackTrace --runInBand",
    "test:unit:verbose": "jest --c jest.unit.config.ts --verbose --runInBand --detectOpenHandles",
    "test:unit:coverage": "jest --c jest.unit.config.ts --silent --noStackTrace --runInBand --coverage",
    "test:unit:watch": "jest --c jest.unit.config.ts --silent --noStackTrace --runInBand --watchAll"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "ioredis": "^5.2.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/ioredis-mock": "^8.2.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "ioredis-mock": "^8.2.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
